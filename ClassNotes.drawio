        <mxCell id="UsOmW5jTE43D732pvYq5-1" value="&lt;div style=&quot;text-align: center;&quot;&gt;&lt;b&gt;Curto-circuito de operadores lógicos&lt;/b&gt;&lt;/div&gt;&lt;font style=&quot;font-size: 24px;&quot;&gt;int x = 10;&lt;br&gt;int y = 20;&lt;br&gt;boolean r = x == 15 &amp;amp;&amp;amp; y++ == 20;&lt;br&gt;return y = 20; &lt;font color=&quot;#ff0080&quot;&gt;(não executou o y)&lt;/font&gt;&lt;br&gt;boolean r = x == 10 &amp;amp;&amp;amp; y++ == 20;&lt;br&gt;return y = 21;&lt;br&gt;boolean r = x == 10 || y++ == 20;&lt;br&gt;return y = 20; &lt;font color=&quot;#ff0080&quot;&gt;(não executou o y)&lt;br&gt;&lt;/font&gt;&lt;br&gt;O operador &amp;amp;&amp;amp; tem precedência com relação ao ||.&lt;br&gt;&lt;/font&gt;" style="rounded=0;whiteSpace=wrap;html=1;strokeColor=#ffffff;fontSize=24;fontColor=#ffffff;fillColor=none;fillStyle=solid;gradientColor=none;glass=0;shadow=0;fontFamily=Courier New;fontStyle=0;align=left;verticalAlign=top;" parent="1" vertex="1">
